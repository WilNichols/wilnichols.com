    Cal("init", "30min", {origin:"https://cal.com"});

    Cal.ns["30min"]("inline", {
      elementOrSelector:"#my-cal-inline",
      config: {"layout":"month_view","theme":"auto"},
      calLink: "wilnichols/30min",
    });

    Cal.ns["30min"]("ui", {"cssVarsPerTheme":{"light":{"cal-brand":"#2e3a4a"},"dark":{"cal-brand":"#ffffff"}},"hideEventTypeDetails":true,"layout":"month_view"});
    </script>
    <!-- Cal inline embed code ends -->
</div>
